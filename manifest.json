{
  "manifest_version": 3,
  "name": "XPath AI Generator",
  "version": "1.0",
  "description": "Generate XPath using GenAI",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "icon.png"
  }

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
